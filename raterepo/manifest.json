{
  "name": "RateRepo",
  "description": "get a rate for a given repository",
  "version": "1.0.0",
  "permissions": [
      "activeTab"
  ],
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png"
  },
  "background": {
    "scripts": ["index.js"],
    "persistent": false
  },
  "author": "github.com/sanix-darker",
  "browser_action": {
      "default_icon": "img/icon-48.png",
      "default_title": "RateRepo"
  },
  "manifest_version": 2,
  "content_scripts": [ {
    "js": [ "index.js" ],
    "matches": [ "http://github.com/*", "https://github.com/*"]
  }]
}
