{
  "name": "Gitomon",
  "description": "------",
  "version": "1.0",
  "permissions": [
      "activeTab"
  ],
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png"
  },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "author": "github.com/osscameroon",
  "browser_action": {
      "default_icon": "img/icon-48.png",
      "default_title": "Gitomon",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_scripts": [ {
    "js": [ "js/background.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}
